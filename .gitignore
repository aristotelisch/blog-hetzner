# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore capistrano db tasks sql backups
/db/*.sql

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
*.log
.env
# /config/database.yml
dump.rdb
/tls

# Ignore application configuration
/config/application.yml

# Ignore .idea folder
.idea/

# Ignore public folder uploads
/public/uploads/
.capistrano
